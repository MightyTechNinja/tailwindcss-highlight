{
  "name": "tailwindcss-highlight",
  "version": "1.0.0",
  "main": "out/extension.js",
  "license": "MIT",
  "publisher": "ellreka",
  "repository": {
    "type": "git",
    "url": "https://github.com/ellreka/tailwindcss-highlight.git"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "tailwindcss highlight",
      "properties": {
        "tailwindcss-highlight.languages": {
          "default": [
            "html",
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact"
          ],
          "title": "language",
          "type": "array"
        },
        "tailwindcss-highlight.utilities": {
          "default": [
            "padding",
            "margin",
            "space"
          ],
          "title": "utilities",
          "type": "array"
        },
        "tailwindcss-highlight.configs": {
          "default": {
            "padding": {
              "regex": "\\b(|-)p(|t|b|r|l|x|y)-[^'\"\\s]+",
              "color": "black",
              "backgroundColor": "rgb(187, 196, 136)"
            },
            "margin": {
              "regex": "\\b(|-)m(|t|b|r|l|x|y)-[^'\"\\s]+",
              "color": "black",
              "backgroundColor": "rgb(173, 134, 91)"
            },
            "space": {
              "regex": "\\b(|-)space-(x|y)-[^'\"\\s]+",
              "color": "black",
              "backgroundColor": "rgb(173, 134, 91)"
            }
          },
          "title": "configs",
          "type": "object"
        }
      }
    }
  },
  "scripts": {
    "dev": "tsc -watch -p ./",
    "package": "vsce package",
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@ellreka/configs": "1.1.12",
    "@types/node": "15.0.2",
    "@types/vscode": "1.55.0",
    "eslint": "7.25.0",
    "prettier": "2.2.1",
    "typescript": "4.2.4",
    "vsce": "1.88.0"
  }
}
