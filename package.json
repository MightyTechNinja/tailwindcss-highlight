{
  "name": "tailwindcss-highlight",
  "version": "1.1.0",
  "main": "out/extension.js",
  "license": "MIT",
  "publisher": "ellreka",
  "repository": {
    "type": "git",
    "url": "https://github.com/ellreka/tailwindcss-highlight.git"
  },
  "keywords": [
    "tailwindcss"
  ],
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.56.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "tailwindcss highlight",
      "properties": {
        "tailwindcss-highlight.languages": {
          "default": [
            "html",
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact",
            "vue"
          ],
          "title": "language",
          "type": "array"
        },
        "tailwindcss-highlight.utilities": {
          "default": [
            "padding",
            "margin",
            "space",
            "border",
            "divide",
            "ring",
            "width",
            "height",
            "flex",
            "grid",
            "order",
            "gridColumn",
            "gridRow",
            "gridAutoColumns",
            "gridAutoRows",
            "gap",
            "justifyContent",
            "justifyItems",
            "justifySelf",
            "alignContent",
            "alignItems",
            "alignSelf",
            "placeContent",
            "placeItems",
            "placeSelf",
            "table",
            "borderCollapse",
            "transition",
            "transitionDuration",
            "transitionTimingFunction",
            "transitionDelay",
            "animation",
            "transform",
            "transformOrigin",
            "scale",
            "rotate",
            "translate",
            "skew"
          ],
          "title": "utilities",
          "type": "array"
        },
        "tailwindcss-highlight.configs": {
          "default": {
            "padding": {
              "regex": "(?<=[:`'\"\\s])(|-)p(|t|b|r|l|x|y)-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#BBC488"
              }
            },
            "margin": {
              "regex": "(?<=[:`'\"\\s])(|-)m(|t|b|r|l|x|y)-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#AD865B"
              }
            },
            "space": {
              "regex": "(?<=[:`'\"\\s])(|-)space-(x|y)-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#AD865B"
              }
            },
            "border": {
              "regex": "(?<=[:`'\"\\s])border(?!-collapse|-separate)(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#DEC38A"
              }
            },
            "divide": {
              "regex": "(?<=[:`'\"\\s])divide(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#DEC38A"
              }
            },
            "ring": {
              "regex": "(?<=[:`'\"\\s])ring(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#DEC38A"
              }
            },
            "width": {
              "regex": "(?<=[:`'\"\\s])(|-)(|max|min)(|-)w-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#a0cade"
              }
            },
            "height": {
              "regex": "(?<=[:`'\"\\s])(|-)(|max|min)(|-)h-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#e6b1a1"
              }
            },
            "flex": {
              "regex": "(?<=[:`'\"\\s])flex(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "grid": {
              "regex": "(?<=[:`'\"\\s])grid(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "order": {
              "regex": "(?<=[:`'\"\\s])(|-)order-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "gridColumn": {
              "regex": "(?<=[:`'\"\\s])col(|-)[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "gridRow": {
              "regex": "(?<=[:`'\"\\s])row(|-)[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "gridAutoColumns": {
              "regex": "(?<=[:`'\"\\s])auto-cols-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "gridAutoRows": {
              "regex": "(?<=[:`'\"\\s])auto-rows-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "gap": {
              "regex": "(?<=[:`'\"\\s])gap-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "justifyContent": {
              "regex": "(?<=[:`'\"\\s])justify-(start|end|center|between|around|evenly)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "justifyItems": {
              "regex": "(?<=[:`'\"\\s])justify-items-(start|end|center|stretch)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "justifySelf": {
              "regex": "(?<=[:`'\"\\s])justify-self-(auto|start|end|center|stretch)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "alignContent": {
              "regex": "(?<=[:`'\"\\s])content-(start|end|center|between|around|evenly)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "alignItems": {
              "regex": "(?<=[:`'\"\\s])items-(start|end|center|baseline|stretch)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "alignSelf": {
              "regex": "(?<=[:`'\"\\s])self-(auto|start|end|center|stretch)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "placeContent": {
              "regex": "(?<=[:`'\"\\s])place-content-(center|start|end|between|around|evenly|stretch)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "placeItems": {
              "regex": "(?<=[:`'\"\\s])place-items-(start|end|center|stretch)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "placeSelf": {
              "regex": "(?<=[:`'\"\\s])place-self-(auto|start|end|center|stretch)",
              "options": {
                "color": "black",
                "backgroundColor": "#B39DDB"
              }
            },
            "table": {
              "regex": "(?<=[:`'\"\\s])(table-fixed|table-auto|table)",
              "options": {
                "color": "black",
                "backgroundColor": "#FFB300"
              }
            },
            "borderCollapse": {
              "regex": "(?<=[:`'\"\\s])border-(collapse|separate)",
              "options": {
                "color": "black",
                "backgroundColor": "#FFB300"
              }
            },
            "transition": {
              "regex": "(?<=[:`'\"\\s])transition(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#9E9E9E"
              }
            },
            "transitionDuration": {
              "regex": "(?<=[:`'\"\\s])duration-[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#9E9E9E"
              }
            },
            "transitionTimingFunction": {
              "regex": "(?<=[:`'\"\\s])ease(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#9E9E9E"
              }
            },
            "transitionDelay": {
              "regex": "(?<=[:`'\"\\s])delay-[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#9E9E9E"
              }
            },
            "animation": {
              "regex": "(?<=[:`'\"\\s])animate-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#FF80AB"
              }
            },
            "transform": {
              "regex": "(?<=[:`'\"\\s])transform(|-)[^`'\"\\s]*",
              "options": {
                "color": "black",
                "backgroundColor": "#FF80AB"
              }
            },
            "transformOrigin": {
              "regex": "(?<=[:`'\"\\s])origin-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#FF80AB"
              }
            },
            "scale": {
              "regex": "(?<=[:`'\"\\s])scale-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#FF80AB"
              }
            },
            "rotate": {
              "regex": "(?<=[:`'\"\\s])(|-)rotate-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#FF80AB"
              }
            },
            "translate": {
              "regex": "(?<=[:`'\"\\s])(|-)translate-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#FF80AB"
              }
            },
            "skew": {
              "regex": "(?<=[:`'\"\\s])(|-)skew-[^`'\"\\s]+",
              "options": {
                "color": "black",
                "backgroundColor": "#FF80AB"
              }
            }
          },
          "title": "configs",
          "type": "object"
        }
      }
    }
  },
  "scripts": {
    "dev": "tsc -watch -p ./",
    "package": "vsce package",
    "vscode:prepublish": "tsc -p ./",
    "publish": "vsce publish",
    "test": "jest"
  },
  "devDependencies": {
    "@ellreka/configs": "1.1.12",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.0",
    "@types/vscode": "1.56.0",
    "eslint": "7.26.0",
    "jest": "26.6.3",
    "prettier": "2.3.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "vsce": "1.88.0"
  }
}
